{
  "name": "whynot.js",
  "version": "0.5.0",
  "description": "Generic VM-based structure matching framework",
  "author": "Stef Busking <stef.busking@gmail.com>",
  "contributors": [
    "Martin Middel <martinmiddel@gmail.com>"
  ],
  "license": "MIT",
  "keywords": [
    "Language",
    "Matching",
    "Regex",
    "Structure"
  ],
  "main": "dist/whynot.js",
  "module": "lib/index.js",
  "scripts": {
    "build:amd": "rimraf lib && tsc --module amd",
    "build:commonjs": "rimraf lib && tsc --module commonjs",
    "build:umd": "rimraf dist && rimraf lib && tsc && rollup -c",
    "docs": "typedoc --out docs --excludePrivate --excludeNotExported src/index.ts",
    "test": "tsc -P test && mocha --recursive test/bin/test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bwrrp/whynot.js"
  },
  "devDependencies": {
    "@types/chai": "^3.5.2",
    "@types/mocha": "^2.2.41",
    "chai": "^3.5.0",
    "mocha": "^3.3.0",
    "rimraf": "^2.6.1",
    "rollup": "^0.41.6",
    "rollup-plugin-babili": "^3.0.0",
    "typedoc": "^0.6.0",
    "typescript": "^2.3.2"
  }
}
